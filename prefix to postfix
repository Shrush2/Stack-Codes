class Solution {
    static String preToPost(String s) {
  if (s == null || s.isEmpty()) {
            throw new IllegalArgumentException("Prefix expression cannot be null or empty");
        }
    Stack<String>st = new Stack<>();



     for(int i=s.length()-1; i >= 0;i--){
         char c = s.charAt(i);
         
         if(Character.isLetterOrDigit(c)){
            st.push(c + "");
         
             
         }else{
              if (st.size() < 2) {
                    throw new IllegalArgumentException("Invalid prefix expression: insufficient operands for operator '" + c + "'");
                }
             String op1 = st.pop();
             String op2 = st.pop();
             String temp = op1 + op2 + c;
             st.push(temp);
         }
         
     }
     if (st.size() != 1) {
            throw new IllegalArgumentException("Invalid prefix expression: too many operands");
        }

     return st.pop();
     }
}
