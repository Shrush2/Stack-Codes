class MyStack {
    class StackNode {
        int data;
        StackNode next;
        StackNode(int a) {
            data = a;
            next = null;
        }
    }
    int size = 0;
    StackNode top;
   

    // Function to push an integer into the stack.
    void push(int a) {
     StackNode element = new StackNode(a);
     element.next = top;
     top = element;
     size++;
    }

    // Function to remove an item from top of the stack.
    int pop() {
        if(top == null){
            return -1;
        }
        StackNode temp = top;
        int topdata = top.data;
        top = top.next;
        size--;
        return topdata;
    }
    
    int size(){
        return size;
    }
    
    boolean isempty(){
        return top == null;
    }
}
